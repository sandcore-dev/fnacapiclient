{
    "name": "sandcore-dev/fnacapiclient",
    "description": "Fnac Marketplace API PHP connector",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.4 | ^8.0",
        "ext-dom": "*",
        "ext-xsl": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "monolog/monolog": "^2.0",
        "symfony/finder": "^5.1",
        "symfony/http-foundation": "^5.1",
        "symfony/serializer": "^5.1",
        "symfony/yaml": "^5.1",
        "laminas/laminas-http": "^2.11",
        "laminas/laminas-uri": "^2.7",
        "laminas/laminas-validator": "^2.13"
    },
    "autoload": {
        "psr-4": {
            "FnacApiClient\\": "src/FnacApiClient/",
            "FnacApiGui\\": "src/FnacApiGui/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "v4.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "laminas/laminas-dependency-plugin": true
        }
    }
}
