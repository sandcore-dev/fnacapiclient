{
    "name": "sandcore-dev/fnacapiclient",
    "description": "Fnac Marketplace API PHP connector",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "ext-dom": "*",
        "ext-xsl": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "monolog/monolog" : "^2.0",
        "symfony/class-loader": "^3.4",
        "symfony/finder" : "^5.1",
        "symfony/http-foundation": "^5.1",
        "symfony/serializer": "^5.1",
        "symfony/yaml" : "^5.1",
        "zendframework/zend-http" : "^2.11",
        "zendframework/zend-uri" : "^2.7",
        "zendframework/zend-validator" : "^2.13"
    },
    "autoload": {
        "psr-4": {
            "FnacApiClient\\": "src/FnacApiClient/",
            "FnacApiGui\\": "src/FnacApiGui/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "v2.x-dev"
        }
    }
}